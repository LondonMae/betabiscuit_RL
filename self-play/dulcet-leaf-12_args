--algo td3 --env hockey --max_episodes 1000000 --save_interval 5000 --batch_size 512 --hidden_sizes_actor [300,300] --hidden_sizes_critic [300,300] --train_iter 2 --update 2 --discount 0.99 --lr 0.0001 --learning_rate_actor 0.0001 --learning_rate_critic 0.0001 --policy_noise 0.2 --noise_clip 0.5 --bc --dense_reward --per --cpu --wandb